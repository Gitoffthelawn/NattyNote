{
  "type": "module",
  "name": "nattynote",
  "version": "2.1.2",
  "description": "Take time-stamped YouTube notes.",
  "author": "Ahmad Alq",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedelq/NattyNote/"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.dev.cjs",
    "prod": "webpack --config ./config/webpack.prod.cjs",
    "serve:firefox": "web-ext run --source-dir ./build/firefox  --target firefox-desktop --start-url https://www.youtube.com/watch?v=Y7dpJ0oseIA",
    "serve:chrome": "web-ext run --source-dir ./build/chrome  --target chromium --start-url https://www.youtube.com/watch?v=Y7dpJ0oseIA",
    "serve:brave:linux": "web-ext run --source-dir ./build/chrome  --target chromium --chromium-binary /usr/bin/brave --start-url https://www.youtube.com/watch?v=Y7dpJ0oseIA",
    "lint": "eslint --fix"
  },
  "engines": {
    "node": ">=18.18.0 <24.0.0"
  },
  "browserslist": [
    "Chrome >= 76",
    "Firefox >= 79"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@types/chrome": "^0.1.24",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "filemanager-webpack-plugin": "^9.0.1",
    "html-minimizer-webpack-plugin": "^5.0.3",
    "html-webpack-plugin": "^5.6.4",
    "json-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.3.14",
    "uglify-js": "^3.19.3",
    "web-ext": "^8.10.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}
